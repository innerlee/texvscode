// 将设置放入此文件中以覆盖默认值和用户设置。
{
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/[^m]*.sty": true,
    "**/m[^m]*.sty": true,
    "*.out": true,
    "*.js": true,
    "*.aux": true,
    "*.dvi": true,
    "*.fls": true,
    "*.bbl": true,
    "*.bst": true,
    "*.brf": true,
    "*.log": true,
    "*.blg": true,
    "*.lof": true,
    "*.lot": true,
    "*.ptp": true,
    "*.toc": true,
    "*.glo": true,
    "*.idx": true,
    "thesis.pdf": true,
    "*(busy)*": true,
    "main.pdf": true,
    "bf.pdf": true,
    "*.vscodeLog": true,
    ".gitignore": true,
    ".vscode": true,
    "*.fdb_latexmk": true,
    "*.synctex": true,
    "*.synctex.gz": true,
    "*.synctex.gz(busy)": true,
    "*.pdfsync": true,
    "*.chktexrc": true,
    "node_modules": true
  }
}
